gle/input/visual-question-answering-computer-vision-nlp/dataset'\nANSWER_SPACE_PATH = f'{DATASET_ROOT_PATH}/answer_space.txt'\nCSV_PATH = f'{DATASET_ROOT_PATH}/data.csv'\nIMAGE_DIR = f'{DATASET_ROOT_PATH}/images'\n\n# Hyperparameters\nBATCH_SIZE = 4  # Reduced from 16 to fit in GPU memory\nLEARNING_RATE = 5e-5  # Lower LR for fine-tuning\nNUM_EPOCHS = 10\nPATIENCE = 3  # Early stopping patience\nGRADIENT_ACCUMULATION_STEPS = 4  # Simulate larger batch size\n\n# ==================== LOAD ANSWER SPACE ====================\nprint(\"Loading answer space...\")\nwith open(ANSWER_SPACE_PATH, 'r') as f:\n    classes = [line.strip() for line in f if line.strip()]\n\nn_classes = len(classes)\nclasses_to_idx = {cls: idx for idx, cls in enumerate(classes)}\nidx_to_classes = {idx: cls for idx, cls in enumerate(classes)}\n\nprint(f\"Number of classes: {n_classes}\")\nprint(f\"Sample classes: {classes[:5]}\")\n\n# ==================== LOAD AND SPLIT DATA ====================\nprint(\"\\nLoading dataset...\")\ndf = pd.read_csv(CSV_PATH)\nall_data = [\n    {\n        'question': q, \n        'answer': a.split(',')[0],  # Take first answer\n        'image_id': i\n    } \n    for q, a, i in zip(df['question'], df['answer'], df['image_id'])\n]\n\nprint(f\"Total samples: {len(all_data)}\")\n\n# Split data\ntrain_data, test_data = train_test_split(all_data, test_size=0.2, random_state=42)\ntrain_data, val_data = train_test_split(train_data, test_size=0.1, random_state=42)\n\nprint(f\"Train: {len(train_data)}, Val: {len(val_data)}, Test: {len(test_data)}\")\n\n# ==================== CUSTOM DATASET ====================\nclass VQADatasetBLIP(Dataset):\n    \"\"\"Dataset for BLIP VQA fine-tuning\"\"\"\n    \n    def __init__(self, data, image_dir, processor, classes_to_idx):\n        self.data = data\n        self.image_dir = image_dir\n        self.processor = processor\n        self.classes_to_idx = classes_to_idx\n    \n    def __len__(self):\n        return len(self.data)\n    \n    def __getitem__(self, idx):\n        item = self.data[idx]\n        img_path = f\"{self.image_dir}/{item['image_id']}.png\"\n        \n        # Load image\n        try:\n            image = Image.open(img_path).convert(\"RGB\")\n        except Exception as e:\n            print(f\"Error loading {img_path}: {e}\")\n            # Return a dummy black image if loading fails\n            image = Image.new('RGB', (224, 224), color='black')\n        \n        question = item[\"question\"]\n        answer = item[\"answer\"]\n        \n        # Process with BLIP processor\n        inputs = self.processor(\n            images=image,\n            text=question,\n            return_tensors=\"pt\",\n            padding=\"max_length\",\n            truncation=True,\n            max_length=32\n        )\n        \n        # Process answer text for generation target\n        answer_inputs = self.processor(\n            text=answer,\n            return_tensors=\"pt\",\n            padding=\"max_length\",\n            truncation=True,\n            max_length=16\n        )\n        \n        # Squeeze batch dimension\n        inputs = {k: v.squeeze(0) for k, v in inputs.items()}\n        \n        # Add labels (answer tokens) for training\n        inputs['labels'] = answer_inputs['input_ids'].squeeze(0)\n        \n        return inputs\n\n# ==================== LOAD BLIP MODEL ====================\nprint(\"\\nLoading BLIP model and processor...\")\ndevice = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\nprint(f\"Using device: {device}\")\n\n# Clear GPU cache\nif torch.cuda.is_available():\n    torch.cuda.empty_cache()\n\nprocessor = BlipProcessor.from_pretrained(\"Salesforce/blip-vqa-base\")\nmodel = BlipForQuestionAnswering.from_pretrained(\"Salesforce/blip-vqa-base\")\n\n# MEMORY OPTIMIZATION: Freeze vision encoder to save memory\nprint(\"\\nFreezing vision encoder to reduce memory usage...\")\nfor param in model.vision_model.parameters():\n    param.requires_grad = False\n\n# Only fine-tune text encoder and decoder\nfor param in model.text_encoder.parameters():\n    param.requires_grad = True\nfor param in model.text_decoder.parameters():\n    param.requires_grad = True\n\nmodel = model.to(device)\n\n# Enable gradient checkpointing to save memory\nmodel.gradient_checkpointing_enable()\n\nprint(\"BLIP model loaded successfully with memory optimizations!\")\n\n# ==================== CREATE DATALOADERS ====================\nprint(\"\\nCreating dataloaders...\")\n\ntrain_dataset = VQADatasetBLIP(train_data, IMAGE_DIR, processor, classes_to_idx)\nval_dataset = VQADatasetBLIP(val_data, IMAGE_DIR, processor, classes_to_idx)\ntest_dataset = VQADatasetBLIP(test_data, IMAGE_DIR, processor, classes_to_idx)\n\ntrain_loader = DataLoader(train_dataset, batch_size=BATCH_SIZE, shuffle=True, num_workers=0, pin_memory=False)\nval_loader = DataLoader(val_dataset, batch_size=BATCH_SIZE, shuffle=False, num_workers=0, pin_memory=False)\ntest_loader = DataLoader(test_dataset, batch_size=BATCH_SIZE, shuffle=False, num_workers=0, pin_memory=False)\n\nprint(\"Dataloaders created!\")\n\n# ==================== TRAINING SETUP ====================\noptimizer = torch.optim.AdamW(model.parameters(), lr=LEARNING_RATE)\nscheduler = torch.optim.lr_scheduler.ReduceLROnPlateau(\n    optimizer, mode='min', factor=0.5, patience=2, verbose=True\n)\n\nhistory = {\n    'train_loss': [],\n    'val_loss': []\n}\n\n# ==================== TRAINING LOOP ====================\nprint(\"\\n\" + \"=\"*50)\nprint(\"STARTING TRAINING\")\nprint(\"=\"*50)\n\nbest_val_loss = float('inf')\nearly_stop_counter = 0\nbest_model_wts = None\n\nfor epoch in range(1, NUM_EPOCHS + 1):\n    print(f\"\\nEpoch {epoch}/{NUM_EPOCHS}\")\n    \n    # ---------- Training Phase ----------\n    model.train()\n    train_loss = 0.0\n    train_batches = 0\n    \n    optimizer.zero_grad()  # Zero gradients at start of epoch\n    \n    for batch_idx, batch in enumerate(train_loader):\n        # Move batch to device\n        batch = {k: v.to(device) for k, v in batch.items()}\n        \n        # Forward pass\n        outputs = model(**batch)\n        loss = outputs.loss\n        \n        # Normalize loss for gradient accumulation\n        loss = loss / GRADIENT_ACCUMULATION_STEPS\n        \n        # Backward pass\n        loss.backward()\n        \n        # Update weights every GRADIENT_ACCUMULATION_STEPS\n        if (batch_idx + 1) % GRADIENT_ACCUMULATION_STEPS == 0:\n            optimizer.step()\n            optimizer.zero_grad()\n            \n            # Clear cache periodically\n            if torch.cuda.is_available():\n                torch.cuda.empty_cache()\n        \n        train_loss += loss.item() * GRADIENT_ACCUMULATION_STEPS  # Denormalize for logging\n        train_batches += 1\n        \n        if (batch_idx + 1) % 50 == 0:\n            print(f\"  Batch {batch_idx + 1}/{len(train_loader)}, Loss: {loss.item() * GRADIENT_ACCUMULATION_STEPS:.4f}\")\n    \n    # Final step if there are remaining gradients\n    if len(train_loader) % GRADIENT_ACCUMULATION_STEPS != 0:\n        optimizer.step()\n        optimizer.zero_grad()\n    \n    avg_train_loss = train_loss / train_batches\n    \n    # ---------- Validation Phase ----------\n    model.eval()\n    val_loss = 0.0\n    val_batches = 0\n    \n    with torch.no_grad():\n        for batch in val_loader:\n            batch = {k: v.to(device) for k, v in batch.items()}\n            outputs = model(**batch)\n            val_loss += outputs.loss.item()\n            val_batches += 1\n            \n            # Clear cache during validation too\n            if torch.cuda.is_available():\n                torch.cuda.empty_cache()\n    \n    avg_val_loss = val_loss / val_batches\n    \n    # Store history\n    history['train_loss'].append(avg_train_loss)\n    history['val_loss'].append(avg_val_loss)\n    \n    print(f\"\\nTrain Loss: {avg_train_loss:.4f}\")\n    print(f\"Val Loss:   {avg_val_loss:.4f}\")\n    \n    # Learning rate scheduling\n    scheduler.step(avg_val_loss)\n    \n    # Early stopping and model saving\n    if avg_val_loss < best_val_loss:\n        best_val_loss = avg_val_loss\n        best_model_wts = copy.deepcopy(model.state_dict())\n        torch.save(model.state_dict(), 'best_blip_vqa_model.pth')\n        early_stop_counter = 0\n        print(\"✓ Validation loss improved - Model saved!\")\n    else:\n        early_stop_counter += 1\n        print(f\"✗ No improvement ({early_stop_counter}/{PATIENCE})\")\n        if early_stop_counter >= PATIENCE:\n            print(f\"\\nEarly stopping triggered at epoch {epoch}\")\n            break\n\n# Load best model\nif best_model_wts is not None:\n    model.load_state_dict(best_model_wts)\nprint(\"\\nTraining complete! Best model loaded.\")\n\n# ==================== EVALUATION ====================\nprint(\"\\n\" + \"=\"*50)\nprint(\"EVALUATION ON TEST SET\")\nprint(\"=\"*50)\n\nmodel.eval()\nall_predictions = []\nall_ground_truths = []\n\nwith torch.no_grad():\n    for batch_idx, batch in enumerate(test_loader):\n        # Move inputs to device (exclude labels)\n        pixel_values = batch['pixel_values'].to(device)\n        input_ids = batch['input_ids'].to(device)\n        attention_mask = batch['attention_mask'].to(device)\n        \n        # Generate answers\n        generated_ids = model.generate(\n            pixel_values=pixel_values,\n            input_ids=input_ids,\n            attention_mask=attention_mask,\n            max_length=16\n        )\n        \n        # Decode predictions\n        predictions = processor.batch_decode(generated_ids, skip_special_tokens=True)\n        \n        # Decode ground truth\n        ground_truths = processor.batch_decode(batch['labels'], skip_special_tokens=True)\n        \n        all_predictions.extend(predictions)\n        all_ground_truths.extend(ground_truths)\n        \n        if (batch_idx + 1) % 20 == 0:\n            print(f\"Processed {batch_idx + 1}/{len(test_loader)} batches\")\n\n# Calculate accuracy\ncorrect = sum([1 for pred, gt in zip(all_predictions, all_ground_truths) \n               if pred.strip().lower() == gt.strip().lower()])\naccuracy = correct / len(all_predictions)\n\nprint(f\"\\nTest Accuracy: {accuracy:.4f} ({accuracy*100:.2f}%)\")\nprint(f\"Correct: {correct}/{len(all_predictions)}\")\n\n# Show sample predictions\nprint(\"\\n\" + \"=\"*50)\nprint(\"SAMPLE PREDICTIONS\")\nprint(\"=\"*50)\nfor i in range(min(10, len(all_predictions))):\n    print(f\"\\nSample {i+1}:\")\n    print(f\"  Question: {test_data[i]['question']}\")\n    print(f\"  Ground Truth: {all_ground_truths[i]}\")\n    print(f\"  Prediction:   {all_predictions[i]}\")\n    print(f\"  Match: {'✓' if all_predictions[i].strip().lower() == all_ground_truths[i].strip().lower() else '✗'}\")\n\n# ==================== PLOT TRAINING HISTORY ====================\nplt.figure(figsize=(10, 5))\nplt.plot(history['train_loss'], label='Train Loss', linewidth=2, marker='o')\nplt.plot(history['val_loss'], label='Val Loss', linewidth=2, marker='s')\nplt.xlabel('Epoch', fontsize=12)\nplt.ylabel('Loss', fontsize=12)\nplt.title('BLIP Fine-tuning: Training History', fontsize=14, fontweight='bold')\nplt.legend(fontsize=10)\nplt.grid(True, alpha=0.3)\nplt.tight_layout()\nplt.savefig('blip_training_history.png', dpi=300, bbox_inches='tight')\nplt.show()\nprint(\"\\n✅ Training history saved as 'blip_training_history.png'\")\n\n# ==================== INFERENCE FUNCTION ====================\ndef predict_vqa_blip(image_path, question, model, processor, device):\n    \"\"\"\n    Predict answer for a single image-question pair using fine-tuned BLIP.\n    \n    Args:\n        image_path: Path to image\n        question: Question text\n        model: Fine-tuned BLIP model\n        processor: BLIP processor\n        device: torch device\n    \n    Returns:\n        answer: Predicted answer string\n    \"\"\"\n    # Load and process image\n    image = Image.open(image_path).convert(\"RGB\")\n    \n    # Process inputs\n    inputs = processor(images=image, text=question, return_tensors=\"pt\")\n    inputs = {k: v.to(device) for k, v in inputs.items()}\n    \n    # Generate answer\n    model.eval()\n    with torch.no_grad():\n        generated_ids = model.generate(**inputs, max_length=16)\n    \n    # Decode answer\n    answer = processor.decode(generated_ids[0], skip_special_tokens=True)\n    return answer\n\n# Test inference\nsample_image = f\"{IMAGE_DIR}/image100.png\"\nsample_question = \"What is in the image?\"\n\nif os.path.exists(sample_image):\n    print(\"\\n\" + \"=\"*50)\n    print(\"SAMPLE INFERENCE\")\n    print(\"=\"*50)\n    answer = predict_vqa_blip(sample_image, sample_question, model, processor, device)\n    print(f\"Question: {sample_question}\")\n    print(f\"Answer: {answer}\")\n\nprint(\"\\n\" + \"=\"*50)\nprint(\"SCRIPT COMPLETED!\")\nprint(\"=\"*50)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-11-08T15:05:40.966200Z","iopub.execute_input":"2025-11-08T15:05:40.966847Z","iopub.status.idle":"2025-11-08T16:54:10.103636Z","shell.execute_reply.started":"2025-11-08T15:05:40.966820Z","shell.execute_reply":"2025-11-08T16:54:10.102882Z"}},"outputs":[{"name":"stderr","text":"/usr/local/lib/python3.11/dist-packages/transformers/utils/generic.py:309: FutureWarning: `torch.utils._pytree._register_pytree_node` is deprecated. Please use `torch.utils._pytree.register_pytree_node` instead.\n  _torch_pytree._register_pytree_node(\n/usr/local/lib/python3.11/dist-packages/transformers/utils/generic.py:309: FutureWarning: `torch.utils._pytree._register_pytree_node` is deprecated. Please use `torch.utils._pytree.register_pytree_node` instead.\n  _torch_pytree._register_pytree_node(\n","output_type":"stream"},{"name":"stdout","text":"GPU Memory before starting: 0.00 GB\nLoading answer space...\nNumber of classes: 582\nSample classes: ['1', '10', '11', '12', '13']\n\nLoading dataset...\nTotal samples: 12468\